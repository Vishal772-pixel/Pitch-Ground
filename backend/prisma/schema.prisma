generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url =env("DATABASE_URL")
}

Model User {
    id String @id @default(cuid())
    name String 
    email String @unique
    password String
    createdAt DateTime @default(now())
    // Now install 2 dependecies =>  npx prisma migrate dev --name init
    //npx prisma generate
}
